{{- $.Scratch.Add "index" slice -}}
{{- range where site.RegularPages "Type" "in" site.Params.mainSections -}}
  {{- $.Scratch.Add "index" (dict "title" .Title "categories" .Params.categories "tags" .Params.tags "contents" .Plain "permalink" .Permalink "date" .Date) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
